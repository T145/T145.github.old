{
  "name": "gitdocs",
  "version": "1.1.4",
  "private": true,
  "scripts": {
    "browser": "npx browserslist@latest --update-db",
    "build": "craco build",
    "predeploy": "craco build",
    "deploy": "gh-pages -d build",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx --quiet",
    "lint-fix": "eslint ./src --ext .js,.jsx,.ts,.tsx --fix",
    "sort": "sort-package-json",
    "start": "craco start",
    "test": "craco test",
    "update": "ncu -u"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
