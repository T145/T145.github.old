    "react-device-detect": "^2.1.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-player": "^2.9.0",
    "react-scripts": "5.0.0",
    "react-swipeable-views": "^0.14.0",
