{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Enzyme",
  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/component/component.tsx"
      },
      "path": "src/components",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": false
    },
    "page": {
      "customTemplates": {
        "component": "templates/component/component.tsx"
      },
      "path": "src/pages",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": false
    },
    "layout": {
      "customTemplates": {
        "component": "templates/component/component.tsx"
      },
      "path": "src/layouts",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": false
    }
  }
}
